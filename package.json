{
  "name": "hono-api-key",
  "version": "0.0.0",
  "description": "Hono middleware to validate API keys via header, query, or custom adapter.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish",
    "examples:basic": "tsx examples/basic.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "hono",
    "middleware",
    "api-key",
    "security"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qutek/hono-api-key.git"
  },
  "bugs": {
    "url": "https://github.com/qutek/hono-api-key/issues"
  },
  "homepage": "https://github.com/qutek/hono-api-key#readme",
  "devDependencies": {
    "@hono/node-server": "^1.13.6",
    "@changesets/cli": "^2.27.7",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.12.0",
    "hono": "^4.5.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "hono": ">=4.0.0"
  }
}
